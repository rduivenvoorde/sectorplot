<?xml version = "1.0"?>
<?xml-stylesheet type="text/xsl" href="plugins.xsl"?>
 <plugins>

   <pyqgis_plugin name="SectorPlot" version="0.6.3">
     <description>Plots sector areas on risk management map</description>
     <homepage>https://github.com/rduivenvoorde/sectorplot</homepage>
     <qgis_minimum_version>2.12</qgis_minimum_version>
     <file_name>SectorPlot.zip</file_name>
     <author_name>RIVM, Marnix de Ridder</author_name>
     <download_url>http://repo.svc.cal-net.nl/repo/rivm/qgis/SectorPlot.zip</download_url>
   </pyqgis_plugin>

   <pyqgis_plugin name="RegenRadar" version="0.2">
     <description>This plugin tests if you have TimeManager plugin installed and active. Then creates a WMS-T layer of current precipitation in The Netherlands, loads this layer into QGIS, and registres it as a TimeLayer in the TimeManager. Then it tries to 'Play' WMS-T from last hour.</description>
     <homepage>https://github.com/rduivenvoorde/regenradar</homepage>
     <qgis_minimum_version>2.12</qgis_minimum_version>
     <file_name>RegenRadar.zip</file_name>
     <author_name>RIVM, Marnix de Ridder</author_name>
     <download_url>http://repo.svc.cal-net.nl/repo/rivm/qgis/RegenRadar.zip</download_url>
   </pyqgis_plugin>

   <pyqgis_plugin name="JRodos" version="0.3.4">
     <description>This plugin shows JRodos model output, measurements and weather data</description>
     <homepage>https://github.com/rduivenvoorde/jrodos</homepage>
     <qgis_minimum_version>2.12</qgis_minimum_version>
     <file_name>JRodos.zip</file_name>
     <author_name>RIVM, Marnix de Ridder</author_name>
     <download_url>http://repo.svc.cal-net.nl/repo/rivm/qgis/JRodos.zip</download_url>
   </pyqgis_plugin>

   <pyqgis_plugin name="TimeManager (RIVM)" version="2.3.2">
     <description>This plugin </description>
     <homepage>https://github.com/rduivenvoorde/timemanager</homepage>
     <qgis_minimum_version>2.12</qgis_minimum_version>
     <file_name>timemanager.zip</file_name>
     <author_name>RIVM, Marnix de Ridder</author_name>
     <download_url>http://repo.svc.cal-net.nl/repo/rivm/qgis/timemanager.zip</download_url>
   </pyqgis_plugin>

 </plugins>
